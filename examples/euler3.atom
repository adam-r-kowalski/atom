problem3 = fn(x: i32, current: i32, cutoff: i32, largest_factor: i32) i32 {
	if {
        current > cutoff { largest_factor }
        x % current == 0 { problem3(x / current, current + 1, cutoff, current) }
        else { problem3(x, current + 1, cutoff, largest_factor) }
	}
}

start = fn() i32 {
	x: i32 = 13195
    cutoff = convert(sqrt(convert(x, f32)), i32)
	problem3(x, 2, cutoff, 0)
}
